# Synchrenity Framework .env

APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost
APP_KEY=base64:SomeRandomKeyHere
APP_TIMEZONE=UTC
APP_LOCALE=en
APP_FALLBACK_LOCALE=en

# Database settings
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=synchrenity
DB_USERNAME=root
DB_PASSWORD=secret

# Mail settings
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@synchrenity.com"
MAIL_FROM_NAME="Synchrenity"

# Cache settings
CACHE_DRIVER=file
SESSION_DRIVER=file
QUEUE_DRIVER=sync

# Logging
LOG_CHANNEL=stack
LOG_LEVEL=debug

# Custom
CUSTOM_OPTION="This is a custom value"
MULTILINE_VAR="This is a multi-line value
that spans multiple lines
and ends here."
EXPANDED_VAR="Database host is ${DB_HOST}"
BOOLEAN_TRUE=true
BOOLEAN_FALSE=false
INTEGER_EXAMPLE=42
FLOAT_EXAMPLE=3.14159
NULL_EXAMPLE=null
